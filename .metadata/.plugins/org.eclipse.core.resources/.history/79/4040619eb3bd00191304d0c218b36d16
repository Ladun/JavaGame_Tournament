package com.ladun.engine;

public class Time {
	private static final float DELTA_TIME = 1.0/60.0;
	private final double UPDATE_CAP = 1.0/60.0;

	public static float getDeltaTime() {
		return DELTA_TIME;
	}

	public static void setDeltaTime(float deltaTime) {
		Time.DELTA_TIME = deltaTime;
	}
	
	
	
}
